{
  "name": "scrumi-react",
  "version": "6.0.0",
  "description": "Keep Track of Tasks",
  "main": "index.js",
  "scripts": {
    "build-client": "webpack --progress --watch",
    "build-service-worker": "babel src/service-worker.js 2>&1 | tee dist/assets/service-worker.js",
    "build-server": "babel src -d build --watch",
    "start-server": "nodemon ./build/server/index.js",
    "dev-all": "npm run build-client & npm run build-server & npm run start-server-nodemon",
    "prestart": "npm run build-client; npm run build-server",
    "start-server-babel": "babel-node ./src/server",
    "start-server-nodemon-babel": "nodemon -e js --exec babel-node ./src/server/index.js",
    "start-server-node": "node ./build/server/index.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "lint-watch": "esw --watch .",
    "test-watch": "jest --watch --testPathIgnorePatterns dbmodels.test.js",
    "clear-jest": "jest --clearCache",
    "jest-config": "jest --showConfig",
    "test": "jest --testPathIgnorePatterns dbmodels.test.js",
    "test-all": "jest --all",
    "test-watch-all": "jest --watchAll --testPathIgnorePatterns dbmodels.test.js",
    "test-coverage": "jest --coverage=true",
    "test-app": "jest --watch app.test.js",
    "test-models": "jest --watch dbmodels.test.js",
    "test-objectives-reducers": "jest --watch objectives-reducers.test.js",
    "test-actions-watch": "jest --watch actions.test.js",
    "test-objectives-mock": "jest --watch objectives-mock.test.js",
    "test-objectives-api": "jest --watch objectives-api.test.js",
    "test-objectives-filter-form": "jest --watch ObjectivesFilterForm.test.js",
    "test-objectives-filter-form-container": "jest --watch ObjectivesFilterFormContainer.test.js"
  },
  "keywords": [
    "React",
    "state",
    "setState",
    "explicitly",
    "passing",
    "props"
  ],
  "author": "John D. Aynedjian <john.david.aynedjian@gmail.com> (http://johndavidaynedjian.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "css-loader": "0.28.7",
    "deep-freeze": "0.0.1",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "enzyme-to-json": "3.0.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "3.0.0",
    "jest": "21.2.1",
    "jest-css-modules": "1.1.0",
    "jest-enzyme": "4.0.0",
    "jest-fetch-mock": "^1.6.6",
    "jwt-simple": "^0.5.6",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "postcss-loader": "2.0.6",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.6.0",
    "webpack-dev-server": "3.1.11"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "express": "4.15.5",
    "httpster": "1.0.3",
    "ignore-styles": "5.0.1",
    "isomorphic-fetch": "2.2.1",
    "log4js": "^2.8.0",
    "logatim": "^0.9.0",
    "pg": "^7.4.3",
    "prop-types": "15.6.0",
    "query-string": "^6.2.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-icons": "2.2.5",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "superagent": "^3.8.3",
    "supertest": "^3.1.0",
    "uuid": "3.1.0",
    "watchman": "^1.0.0"
  },
  "jest": {
    "setupFiles": [
      "./__tests__/shim.js",
      "./__tests__/global.js"
    ],
    "modulePathIgnorePatterns": [
      "shim.js",
      "global.js",
      "build",
      "~",
      ".swp",
      ".tmp",
      "4913"
    ],
    "testPathIgnorePatterns": [
      "~",
      ".swp",
      ".tmp",
      "4913",
      "node_modules"
    ],
    "verbose": true,
    "automock": false,
    "clearMocks": true,
    "bail": false,
    "silent": false,
    "collectCoverage": false,
    "notify": false,
    "collectCoverageFrom": [
      "src/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
